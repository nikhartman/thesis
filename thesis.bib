file = {:Users/nik/Documents/Mendeley Desktop/J\o rgensen et al. - Unknown - 2007.pdf:pdf},
isbn = {1745-2481},
issn = {1745-2473},
number = {JULY},
pages = {2--5},
title = {{Singlet-Triplet Physics and Shell Filling in Carbon Nanotube Double Quantum Dots}},
url = {http://arxiv.org/abs/0711.3245},
volume = {4},
year = {2007}
}
