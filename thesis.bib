keywords = {1,4,carbon nanotube,cnts,disorder provide a promising,electron transport,ltraclean carbon nanotubes,nanodevice,nanoelectronics,platform to manipulate,quantum dot,single electron or hole,spins for quantum information,that are free from},
month = sep,
number = {9},
pages = {4522--6},
pmid = {23962122},
title = {{Ultraclean single, double, and triple carbon nanotube quantum dots with recessed Re bottom gates.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23962122},
volume = {13},
year = {2013}
